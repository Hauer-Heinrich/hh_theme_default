{
    "name": "hauerheinrich/hh-theme-default",
    "type": "typo3-cms-extension",
    "description": "",
    "authors": [
        {
            "name": "Christian Hackl",
            "email": "web@hauer-heinrich.de"
        }
    ],
    "version": "0.1.0",
    "license": "GPL-2.0",
    "keywords": ["TYPO3 CMS", "Theme", "Distribution"],
    "replace": {
        "hh_theme_default": "self.version",
        "typo3-ter/hh-theme-default": "self.version"
    },
    "require": {
        "typo3/cms-core": "^11.5.0 || ^11.5.99",
        "typo3/cms-fluid": "^11.5.0 || ^11.5.99",
        "typo3/cms-frontend": "^11.5.0 || ^11.5.99",
        "typo3/cms-fluid-styled-content": "^11.5.0 || ^11.5.99",
        "typo3/cms-rte-ckeditor": "",
        "typo3/cms-scheduler": "",
        "georgringer/news": "",
        "friendsoftypo3/tt-address": "",
        "in2code/powermail": "",
        "hauerheinrich/hh_slider": "",
        "hauerheinrich/hh_seo": ""
    },
    "suggest": {
        "groundstack/gs-monitor-provider": "",
        "hauerheinrich/hh-widget-hhnews": ""
    },
    "autoload": {
        "psr-4": {
            "HauerHeinrich\\HhThemeDefault\\": "Classes"
        }
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "hh_theme_default"
        }
    }
}
